<h1>Reassembling: <%= @hubot.title %></h1>
<div class="row-fluid">
  <%= form_for @hubot, html: { class: 'form form-inline' } do |f| %>
    <%= render 'hubot_fields', f: f %>
    <div class="form-actions">
      <%= f.submit 'Update', class: 'btn btn-primary' %>
      <%= link_to "Back", @hubot, class: 'btn pull-right' %>
    </div>
  <% end %>
</div>
